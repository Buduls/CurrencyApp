<div ng-if="result.Success">
    <table>
        <thead>
            <tr>
                <th>@Resources.ExchangeRates.Tbl_Heading_Name</th>
                <th>@Resources.ExchangeRates.Tbl_Heading_Rate</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="currency in result.Data">
                <td>{{currency.Name}}</td>
                <td>{{currency.Rate}}</td>
            </tr>
        </tbody>
    </table>
</div>
<div ng-if="!result.Success">
    <h2>@Resources.ExchangeRates.Err_DataLoading</h2>
</div>